{
    "name": "asemalalami/laravel-advanced-filter",
    "description": "Laravel Advanced Filter",
    "keywords": [
        "laravel",
        "filter",
        "filters"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Asem Alalami",
            "email": "asem.almi@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "AsemAlalami\\LaravelAdvancedFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsemAlalami\\LaravelAdvancedFilter\\Test\\": "tests"
        }
    },
    "require": {
        "ext-json": "*",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "orchestra/testbench": "5.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AsemAlalami\\LaravelAdvancedFilter\\AdvancedFilterServiceProvider"
            ]
        }
    }
}
